{
  "env": {
    "start": {
      "devtool": "eval-source-map",
      "presets": [
        "eslint",
        "hmr",
        "react",
        "dev:css",
        "images",
        "json"
      ]
    },
    "gh-pages": {
      "output": {
        "path": "./gh-pages",
        "filename": "[name].[chunkhash].js",
        "chunkFilename": "[chunkhash].js"
      },
      "presets": [
        "production:vendor",
        "production:css",
        "react",
        "images",
        "json"
      ]
    }
  }
}
