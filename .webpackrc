{
  "presets": [
    "images",
    "json",
    "react"
  ],
  "common": {
    "dist": {
      "devtool": "source-map",
      "output": {
        "path": "./dist",
        "libraryTarget": "umd",
        "library": "Boilerplate"
      },
      "entry": "./src",
      "externals": {
        "react": {
          "commonjs": "react",
          "commonjs2": "react",
          "amd": "React",
          "root": "React"
        }
      }
    }
  },
  "env": {
    "start": {
      "devtool": "eval-source-map",
      "presets": [
        "eslint",
        "hmr",
        "dev:css"
      ]
    },
    "gh-pages": {
      "output": {
        "path": "./gh-pages",
        "filename": "[name].[chunkhash].js",
        "chunkFilename": "[chunkhash].js"
      },
      "presets": [
        "production:vendor",
        "production:css",
        "minify"
      ]
    },
    "tdd": {
      "presets": [
        "eslint"
      ]
    },
    "test": {
      "presets": [
        "eslint"
      ]
    },
    "dist": {
      "output": {
        "filename": "boilerplate.js"
      }
    },
    "dist:min": {
      "output": {
        "filename": "boilerplate.min.js"
      },
      "presets": [
        "minify"
      ]
    }
  }
}
